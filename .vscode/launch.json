{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "app.main"
        },
        {
            "name": "FastAPI: Dev",
            "type": "debugpy",
            "request": "launch",
            "module": "fastapi",
            "args": [
                "dev",
                "app/main.py"
            ]
        },
        {
            "name": "FastAPI: Production",
            "type": "debugpy",
            "request": "launch",
            "module": "fastapi",
            "args": [
                "run",
                "app/main.py"
            ]
        },
        {
            "name": "uvicorn",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--reload",
                "--port",
                "8000"
            ],
            "jinja": true
        }
    ]
}